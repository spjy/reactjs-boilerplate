{
  "name": "reactjs-boilerplate",
  "version": "1.0.0",
  "description": "React with Webpack, Babel",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && copy src\\index.html dist\\index.html && webpack-dev-server --content-base src --inline --hot --port 8081",
    "build:prod": "webpack -d && copy src\\index.html dist\\index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Spencer",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.25.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  }
}
